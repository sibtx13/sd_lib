CXXFLAGS := -iquote user $(filter-out -nostdinc++ -Istdinc, $(CXXFLAGS)) -msse

$(O)/utest: $(O)/kernel/crange.o \
            $(O)/kernel/gc.o \
            $(O)/kernel/rnd.o \
            $(O)/user/umain.o
	@echo "  LD     $@"
	$(Q)mkdir -p $(@D)
	$(Q)$(CXX) -o $@ $^ -ljemalloc -lpthread -lrt

.PRECIOUS: $(O)/user/%.o
-include $(O)/user/*.d

ALL := $(O)/utest

